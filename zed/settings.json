{
  "auto_install_extensions": {
    "basher": true,
    "codebook": true,
    "crates-lsp": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "git-firefly": true,
    "graphql": true,
    "html": true,
    "http": true,
    "julia": true,
    "just": true,
    "just-lsp": true,
    "lua": true,
    "make": true,
    "marksman": true,
    "mermaid": true,
    "oxc": true,
    "package-swift-lsp": true,
    "postgres-language-server": true,
    "proto": true,
    "ruff": true,
    "sql": true,
    "sqruff": true,
    "swift": true,
    "toml": true,
    "wit": true
  },
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Mirage"
  },
  "icon_theme": "Zed (Default)",
  "base_keymap": "Atom",
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "buffer_font_size": 14,
  "ui_font_size": 14,
  "vim_mode": true,
  "confirm_quit": true,
  "restore_on_startup": "none",
  "auto_signature_help": true,
  "relative_line_numbers": true,
  "inlay_hints": {
    "enabled": true
  },
  "minimap": {
    "show": "auto"
  },
  "git_panel": {
    "dock": "right"
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware"
  },
  "tabs": {
    "git_status": true,
    "file_icons": true,
    "show_diagnostics": "errors"
  },
  "preview_tabs": {
    "enable_preview_from_file_finder": true,
    "enable_preview_from_code_navigation": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": true,
  "vim": {
    "use_system_clipboard": "on_yank",
    "use_smartcase_find": true
  },
  "terminal": {
    "font_family": "FiraCode Nerd Font Mono",
    "env": {
      "EDITOR": "zed --wait"
    }
  },
  "file_types": {
    "Dockerfile": ["Dockerfile", "Dockerfile.*"],
    "JSON": ["json", "jsonc", "*.code-snippets"]
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**/out",
    "**/dist",
    "**/target",
    "**/.vscode",
    "**/node_modules"
  ],
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "default_profile": "ask",
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    }
  },
  "languages": {
    "YAML": {
      "formatter": "language_server",
      "format_on_save": "off"
    },
    "Python": {
      "format_on_save": { "language_server": { "name": "ruff" } },
      "formatter": { "language_server": { "name": "ruff" } },
      "language_servers": ["pyright", "ruff"]
    }
  },
  "lsp": {
    "jdtls": {
      "initialization_options": {
        "bundles": [],
        "workspaceFolders": [],
        "settings": {
          "java": {
            "home": "/Library/Java/JavaVirtualMachines/graalvm-21.jdk/Contents/Home",
            "errors": {
              "incompleteClasspath": {
                "severity": "warning"
              }
            },
            "configuration": {
              "updateBuildConfiguration": "interactive",
              "maven": {
                "userSettings": null
              }
            },
            "trace": {
              "server": "verbose"
            },
            "import": {
              "gradle": {
                "enabled": true
              },
              "maven": {
                "enabled": true
              },
              "exclusions": [
                "**/node_modules/**",
                "**/.metadata/**",
                "**/archetype-resources/**",
                "**/META-INF/maven/**",
                "/**/test/**"
              ]
            },
            "jdt": {
              "ls": {
                "lombokSupport": {
                  "enabled": true
                },
                "protobufSupport": {
                  "enabled": true
                }
              }
            },
            "referencesCodeLens": {
              "enabled": false
            },
            "signatureHelp": {
              "enabled": true
            },
            "implementationsCodeLens": {
              "enabled": false
            },
            "format": {
              "enabled": true
            },
            "saveActions": {
              "organizeImports": false
            },
            "contentProvider": {
              "preferred": null
            },
            "autobuild": {
              "enabled": true
            },
            "codeGeneration": {
              "generateComments": true,
              "toString": {
                "codeStyle": "STRING_FORMAT"
              }
            },
            "completion": {
              "enabled": true,
              "favoriteStaticMembers": [
                "org.junit.Assert.*",
                "org.junit.Assume.*",
                "org.junit.jupiter.api.Assertions.*",
                "org.junit.jupiter.api.Assumptions.*",
                "org.junit.jupiter.api.DynamicContainer.*",
                "org.junit.jupiter.api.DynamicTest.*",
                "org.mockito.Mockito.*",
                "org.mockito.ArgumentMatchers.*",
                "org.mockito.Answers.*"
              ],
              "filteredTypes": [
                "java.awt.*",
                "com.sun.*",
                "sun.*",
                "jdk.*",
                "org.graalvm.*",
                "io.micrometer.shaded.*"
              ],
              "guessMethodArguments": true,
              "importOrder": ["#", "java", "javax", "org", "com"]
            },
            "progressReport": {
              "enabled": false
            }
          }
        }
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        },
        "check": {
          "command": "clippy",
          "workspace": true
        }
      }
    },
    "vtsls": {
      "settings": {
        "javascript": {
          "inlayHints": {
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "enumMemberValues": {
              "enabled": true
            }
          }
        },
        "typescript": {
          "inlayHints": {
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "enumMemberValues": {
              "enabled": true
            }
          }
        }
      }
    },
    "yaml-language-server": {
      "settings": {
        "yaml": {
          "format": {
            "enable": true,
            "singleQuote": true
          }
        }
      }
    }
  }
}

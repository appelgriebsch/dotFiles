{
  "icon_theme": "Zed (Default)",
  "auto_install_extensions": {
    "basher": true,
    "crates-lsp": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "git-firefly": true,
    "graphql": true,
    "html": true,
    "http": true,
    "julia": true,
    "just": true,
    "lua": true,
    "make": true,
    "marksman": true,
    "mermaid": true,
    "postgres-language-server": true,
    "proto": true,
    "ruff": true,
    "sql": true,
    "sqruff": true,
    "swift": true,
    "toml": true,
    "wit": true
  },
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Mirage"
  },
  "base_keymap": "SublimeText",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "buffer_font_size": 14,
  "ui_font_size": 14,
  "vim_mode": true,
  "confirm_quit": true,
  "restore_on_startup": "none",
  "auto_signature_help": true,
  "relative_line_numbers": true,
  "inlay_hints": {
    "enabled": true
  },
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "o1"
    },
    "version": "2"
  },
  "tabs": {
    "git_status": true,
    "file_icons": true
  },
  "preview_tabs": {
    "enable_preview_from_file_finder": true,
    "enable_preview_from_code_navigation": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": true,
  "vim": {
    "use_multiline_find": true,
    "use_smartcase_find": true
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        },
        "check": {
          "command": "clippy",
          "workspace": true
        }
      }
    },
    "vtsls": {
      "settings": {
        "javascript": {
          "inlayHints": {
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "enumMemberValues": {
              "enabled": true
            }
          }
        },
        "typescript": {
          "inlayHints": {
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "enumMemberValues": {
              "enabled": true
            }
          }
        }
      }
    }
  }
}
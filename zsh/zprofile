[[ -f ~/.cargo/env ]] && source "$HOME/.cargo/env"
[[ -f /opt/homebrew/bin/brew ]] && eval "$(/opt/homebrew/bin/brew shellenv)"
[[ -f /opt/homebrew/bin/fnm ]] && eval "$(/opt/homebrew/bin/fnm env)"
[[ -f ~/Projects/platform/tools-k8s/login/k8s-login ]] && source "$HOME/Projects/platform/tools-k8s/login/k8s-login"

export PATH="$HOME/.local/bin:$HOME/.m2/bin:$PATH"
export JAVA_HOME="/Library/Java/JavaVirtualMachines/graalvm-17.jdk/Contents/Home/"
export DOCKER_HOST="unix://$HOME/.socktainer/container.sock"